#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01242B70 .scope module, "xor_gate_tb" "xor_gate_tb" 2 1;
 .timescale 0 0;
v01244B28_0 .var "a", 0 0;
v01244B80_0 .var "b", 0 0;
v012489F8_0 .net "y", 0 0, L_01242C80; 1 drivers
S_01242BF8 .scope module, "dut" "xor_gate" 2 4, 3 1, S_01242B70;
 .timescale 0 0;
L_01242C80 .functor XOR 1, v01244B28_0, v01244B80_0, C4<0>, C4<0>;
v01248D38_0 .net "a", 0 0, v01244B28_0; 1 drivers
v01244A78_0 .net "b", 0 0, v01244B80_0; 1 drivers
v01244AD0_0 .alias "y", 0 0, v012489F8_0;
    .scope S_01242B70;
T_0 ;
    %set/v v01244B28_0, 0, 1;
    %set/v v01244B80_0, 0, 1;
    %delay 5, 0;
    %set/v v01244B28_0, 0, 1;
    %set/v v01244B80_0, 1, 1;
    %delay 5, 0;
    %set/v v01244B28_0, 1, 1;
    %set/v v01244B80_0, 0, 1;
    %delay 5, 0;
    %set/v v01244B28_0, 1, 1;
    %set/v v01244B80_0, 1, 1;
    %delay 5, 0;
    %end;
    .thread T_0;
    .scope S_01242B70;
T_1 ;
    %vpi_call 2 14 "$monitor", $time, " a = %b, b = %b, y = %b", v01244B28_0, v01244B80_0, v012489F8_0;
    %end;
    .thread T_1;
    .scope S_01242B70;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "xor_gate.vcd";
    %vpi_call 2 19 "$dumpvars", 1'sb0, S_01242B70;
    %end;
    .thread T_2;
