#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01302B70 .scope module, "or_gate_tb" "or_gate_tb" 2 1;
 .timescale 0 0;
v01308BC0_0 .var "a", 0 0;
v01304A78_0 .var "b", 0 0;
v01304AD0_0 .net "y", 0 0, L_01302C80; 1 drivers
S_01302BF8 .scope module, "dut" "or_gate" 2 4, 3 3, S_01302B70;
 .timescale 0 0;
L_01302C80 .functor OR 1, v01308BC0_0, v01304A78_0, C4<0>, C4<0>;
v01308AB8_0 .net "a", 0 0, v01308BC0_0; 1 drivers
v01308B10_0 .net "b", 0 0, v01304A78_0; 1 drivers
v01308B68_0 .alias "y", 0 0, v01304AD0_0;
    .scope S_01302B70;
T_0 ;
    %set/v v01308BC0_0, 0, 1;
    %set/v v01304A78_0, 0, 1;
    %delay 5, 0;
    %set/v v01308BC0_0, 0, 1;
    %set/v v01304A78_0, 1, 1;
    %delay 5, 0;
    %set/v v01308BC0_0, 1, 1;
    %set/v v01304A78_0, 0, 1;
    %delay 5, 0;
    %set/v v01308BC0_0, 1, 1;
    %set/v v01304A78_0, 1, 1;
    %delay 5, 0;
    %end;
    .thread T_0;
    .scope S_01302B70;
T_1 ;
    %vpi_call 2 14 "$monitor", $time, " a = %b, b = %b, y = %b", v01308BC0_0, v01304A78_0, v01304AD0_0;
    %end;
    .thread T_1;
    .scope S_01302B70;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "or_gate.vcd";
    %vpi_call 2 19 "$dumpvars", 1'sb0, S_01302B70;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
